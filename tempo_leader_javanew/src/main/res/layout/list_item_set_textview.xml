<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/all_item_set_textview"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rect">

    <TextView
        android:id="@+id/view_time"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/time_item_set_textview"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/time_item_set_textview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:padding="4dp"
        android:paddingStart="15dp"
        android:text="@string/_3"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view_reps"
        app:layout_constraintStart_toEndOf="@+id/view_time"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/view_reps"
        android:layout_width="20dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/reps_item_set_textview"
        app:layout_constraintStart_toEndOf="@+id/time_item_set_textview"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/reps_item_set_textview"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:padding="4dp"
        android:paddingStart="15dp"
        android:text="@string/_5"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view_mark"
        app:layout_constraintStart_toEndOf="@+id/view_reps"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/view_mark"
        android:layout_width="20dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mark_item_set_textview"
        app:layout_constraintStart_toEndOf="@+id/reps_item_set_textview"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mark_item_set_textview"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:gravity="center"
        android:padding="4dp"
        android:paddingStart="10dp"
        android:text="@string/_0"
        android:textColor="#0512c5"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view_mark_end"
        app:layout_constraintStart_toEndOf="@+id/view_mark"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/view_mark_end"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mark_item_set_textview"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>